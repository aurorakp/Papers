<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.papers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SSN_Activity"
            android:label="@string/title_activity_ssn_"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.papers.MainActivity" />
        </activity>
        <activity
            android:name=".idNYC_Activity"
            android:label="@string/title_activity_idNYC_"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.papers.MainActivity" />
        </activity>
        <activity
            android:name=".BirthCert_Activity"
            android:label="BirthCert_Activity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.papers.MainActivity" />
        </activity>
        <activity
            android:name=".DMV_Activity"
            android:label="DMV_Activity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.papers.MainActivity" />
        </activity>
        <activity
            android:name=".Passport_Activity"
            android:label="@string/title_activity_passport_"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
